  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "demo.start": "ng serve",
    "demo.build": "ng build --prod",
    "demo.test": "ng test",
    "demo.lint": "ng lint",
    "demo.e2e": "ng e2e",
    "demo.pretty": "prettier --config ./.prettierrc --write ./src/**/*.ts",
    "deploy": "ng build --base-href=/ngx-md/ --prod && gh-pages -d dist/ngx-md-demo",
    "lib.start": "ng serve ngx-md",
    "lib.build": "ng build ngx-md",
    "lib.test": "ng test ngx-md",
    "lib.lint": "ng lint ngx-md",
    "lib.pretty": "prettier --config ./.prettierrc --write ./projects/ngx-md/src/**/*.ts",
    "lib.prettier.watch": "onchange 'projects/ngx-md/src**/*.ts' -- prettier --write {{changed}}",
    "prepublishOnly": "npm run lib.build && npm version patch && node ./scripts/pre-publish-hook.js && cd dist/ngx-md && npm publish && cd ../../",
    "demo.prettier.watch": "onchange 'src/app/**/*.ts' -- prettier --write {{changed}}",
    "ci:travis": "npm run lib.lint && ng test ngx-md --watch=false && npm run lib.build"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
