{
  "name": "angular2-markdown",
  "version": "1.6.0",
  "license": "MIT",
  "angular-cli": {},
  "main": "bundles/angular2-markdown.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "ng",
    "angular",
    "angular2",
    "markdown",
    "prismjs"
  ],
  "author": "Dimpu Aravind Buddha <buddhaaravind@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dimpu/angular2-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/dimpu/angular2-markdown/issues"
  },
  "homepage": "https://github.com/dimpu/angular2-markdown#readme",
  "dependencies": {
    "marked": "^0.3.6",
    "prismjs": "^1.6.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test -sr",
    "lint": "exit 0",
    "e2e": "protractor",
    "typedoc": "typedoc --exclude '**/*.spec.ts' ./src/",
    "build": "ngm build -p src --clean",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "lite-server": "lite-server -c demo/bs-config.json",
    "quick-start-demo": "lite-server demo/quick-start  --entry-file=index.html --port=8080",
    "demo.serve": "run-p build.watch link start",
    "demo.gh-pages": "run-s build demo.gh-pages.build demo.deploy",
    "demo.build": "ng build -prod --aot",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo.gh-pages.build": "ng build -prod --aot -bh '/angular2-markdown/'",
    "demo-beta-deploy": "gh-pages -d demo/dist -r git@github.com:dimpu/angular2-markdown.git -b gh-pages",
    "link": "ngm link -p src --here",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "flow.github-release": "conventional-github-releaser -p angular",
    "version": "npm run flow.changelog && git add -A",
    "test-cross": "SAUCE=true ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "pree2e": "webdriver-manager update",
    "e2e-cross": "SAUCE=true npm run e2e"
  },
  "private": false,
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "^2.4.0 || ^4.0.3",
    "@angular/compiler": "^2.4.0 || ^4.0.3",
    "@angular/compiler-cli": "^4.0.3",
    "@angular/core": "^2.4.0 || ^4.0.3",
    "@angular/forms": "^2.4.0 || ^4.0.3",
    "@angular/http": "^2.4.0 || ^4.0.3",
    "@angular/platform-browser": "^2.4.0 || ^4.0.3",
    "@angular/platform-browser-dynamic": "^2.4.0 || ^4.0.3",
    "@angular/router": "^2.4.0 || ^4.0.3",
    "@types/jasmine": "^2.5.47",
    "@types/marked": "0.0.28",
    "@types/node": "^6.0.70",
    "@types/prismjs": "^1.6.2",
    "codelyzer": "^2.0.1",
    "conventional-github-releaser": "^1.1.3",
    "core-js": "^2.4.1",
    "gh-pages": "^0.12.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "lite-server": "^2.3.0",
    "marked-extras": "^0.2.4",
    "ngm-cli": "^0.4.4",
    "npm-run-all": "^4.0.2",
    "protractor": "^5.1.1",
    "ts-node": "^2.1.2",
    "tslint": "^4.5.1",
    "typescript": "^2.2.2",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.6 || ^0.8.5"
  }
}
